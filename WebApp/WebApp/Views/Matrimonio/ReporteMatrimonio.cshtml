@model  WebApp.Models.ViewModels.MatrimonioViewModel

@{
    ViewBag.Title = "ReporteMatrimonio";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Constancia Matrimonio</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<body  style="background: white !important"  >
    <div align="center">
        <div class="divTable" style="width: 100%;">
            <div class="divTableBody" align="center">
                <div class="divTableRow">
                    <div align="center" class="divTableCell"><h3 style="text-transform: uppercase; margin-bottom: -8px;"><img src="~/Content/public/headerMatrimonio.JPG" style="height: 50%;width: 80%;" alt=""> <br></h3> </div>
                </div>
            </div>
        </div>
        <div align="center" style="width: 85%;">
            <br>
            <div class="divTable" style="width: 100%;border: 1px solid #ffffff;/* border-radius: 5px; */">
                <div class="divTableBody">
                    <div class="divTableRow">
                        <div class="divTableCell" style="text-transform: uppercase; font-size: 11px; text-align: -webkit-center;">El Infrascrito de esta parroquia, certifica que en el libro de Matrimonio:</div>
                    </div>
                </div>
            </div>
            <div class="divTable">
                <div class="divTableBody">
                    <div class="divTableRow">
                        <div style="/* font-family: Arial; */text-transform: uppercase;font-size: 13px;/* text-align: right; *//* text-align: left; */" class="divTableCell2">&nbsp;Libro: </div>
                        <div class="divTableCell2" style="/* border-left: 1px solid #d0cfcf; */font-family: Arial;font-size: 13px;border-bottom: 1px solid #d0cfcf;border-radius: 15px;/* text-align: left; */width: 15%;">1</div>
                        <div style="/* font-family: Arial; */text-transform: uppercase;font-size: 13px;/* text-align: right; *//* text-align: left; */" class="divTableCell2">&nbsp;Folio:</div>
